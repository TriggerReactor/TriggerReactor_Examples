IF trigger == "open"
	WHILE true
		//sky
		index = 0
		address = "127.0.0.1:25501"
		message = "&a>> 클릭 시 스카이/야생 서버 이동"
		#CALL "Bungee:ItemServerInfo"
		//war
		index = 2
		address = "127.0.0.1:25502"
		message = "&a>> 클릭 시 전쟁 서버 이동"
		#CALL "Bungee:ItemServerInfo"
		//mini
		index = 4
		address = "127.0.0.1:25503"
		message = "&a>> 클릭 시 미니게임 서버 이동"
		#CALL "Bungee:ItemServerInfo"
		#WAIT 1
		
		//sky
		index = 0
		address = "127.0.0.1:25501"
		message = "&f>> 클릭 시 스카이/야생 서버 이동"
		#CALL "Bungee:ItemServerInfo"
		//war
		index = 2
		address = "127.0.0.1:25502"
		message = "&f>> 클릭 시 전쟁 서버 이동"
		#CALL "Bungee:ItemServerInfo"
		//mini
		index = 4
		address = "127.0.0.1:25503"
		message = "&f>> 클릭 시 미니게임 서버 이동"
		#CALL "Bungee:ItemServerInfo"
