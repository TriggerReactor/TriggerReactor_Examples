IF trigger == "click"
	IF isCoolddown
		#MESSAGE "&6조금 천천히 눌러주세요."
		#STOP
	ENDIF
	
	isCoolddown = true
	
	IF slot == 0
		#SERVER "skyblock"
		#BROADCAST "&8[&6서버이동&8]&7 &6"+player.getName()+" &7-> &6SkyBlock"
	ENDIF
	IF slot == 2
		//#SERVER "war"
		//#BROADCAST "&8[&6서버이동&8]&7 &6"+player.getName()+" &7-> &6War"
	ENDIF
	IF slot == 4
		#SERVER "minigames"
		#BROADCAST "&8[&6서버이동&8]&7 &6"+player.getName()+" &7-> &6MiniGames"
	ENDIF
	
	#WAIT 1
	isCoolddown = false
ENDIF